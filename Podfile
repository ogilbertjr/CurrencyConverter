use_frameworks!

target 'Monies' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  
	pod 'Alamofire', '~> 4.4'
  

  # Pods for Monies

  target 'MoniesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MoniesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
